<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>ITCH II @((ViewBag.Title != null) ? " - " + ViewBag.Title : "")</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,300,600'
        rel='stylesheet' type='text/css'>
    <!-- Stylesheets -->
    <link href="@Url.Content("~/style/bootstrap.css")" rel="stylesheet">
    <link rel ="stylesheet" href="@Url.Content("~/style/font-awesome.css")">
    <link href="@Url.Content("~/style/prettyPhoto.css")" rel="stylesheet">
    <!-- Parallax slider -->
    <link rel="stylesheet" href="@Url.Content("~/style/slider.css")">
    <link href="@Url.Content("~/style/style.css")" rel="stylesheet">
    <!-- Estilo oficial -->
    <link href="@Url.Content("~/style/oficial.css")" rel="stylesheet">
    <!-- HTML5 Soporte IE -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/js/html5shim.js")"></script>
    <![endif]-->
    @*<!-- Favicon -->
        <link rel="shortcut icon" href="@Url.Content("~/img/favicon/favicon.png")">*@

    <!-- JQuery -->
    <script type='text/javascript' src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>

</head>
<body>

    <!-- Header/ -->
    @Html.Partial("~/Views/Shared/WebParts/_Header.cshtml")
    <!-- /Header -->
    
    <!-- Navigation bar/-->
    @Html.Partial("~/Views/Shared/WebParts/_NavigationBar.cshtml")
    <!-- /Navigation bar-->

    <div class="content">
        @RenderBody()
    </div>
    
    <!-- Social/ -->
    @Html.Partial("~/Views/Shared/WebParts/_Social.cshtml")
    <!-- /Social -->
    
    <!-- Footer/ -->
    @Html.Partial("~/Views/Shared/WebParts/_Footer.cshtml")
    <!-- /Footer -->
    

    <!-- JS -->
    <script src="@Url.Content("~/js/bootstrap.js")"></script>
    <script src="@Url.Content("~/js/jquery.isotope.js")"></script>
    <!-- Isotope for gallery -->
    <script src="@Url.Content("~/js/jquery.prettyPhoto.js")"></script>
    <!-- prettyPhoto for images -->
    <script src="@Url.Content("~/js/jquery.cslider.js")"></script>
    <!-- Parallax slider -->
    <script src="@Url.Content("~/js/modernizr.custom.28468.js")"></script>
    <script src="@Url.Content("~/js/filter.js")"></script>
    <!-- Filter for support page -->
    <script src="@Url.Content("~/js/cycle.js")"></script>
    
    <!-- Easing -->
    <script src="@Url.Content("~/js/custom.js")"></script>
</body>
</html>
